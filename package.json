{
  "name": "simka",
  "version": "1.0.0",
  "description": "Sistem Informasi Manajemen Kredit & Agunan - Credit and Collateral Management System",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "vercel-build": "cd client && npm install --legacy-peer-deps && npm run build && cd .. && rm -rf build && mv client/build build",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "setup": "npm install && npm run install-server && npm run install-client"
  },
  "keywords": [
    "banking",
    "credit",
    "collateral",
    "management",
    "indonesia"
  ],
  "author": "SIMKA Development Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/realtime-js": "^2.74.0",
    "@supabase/supabase-js": "^2.74.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "react-scripts": "^5.0.1",
    "sequelize": "^6.32.1",
    "sql.js": "^1.13.0",
    "sqlite3": "^5.1.6",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}